	height : 30px;
	background :  rgba(6, 6, 6, 186);
	color : #aaaaaa;
	border-bottom : 1px solid #c8b029;
	border-left : 5px solid #c8b029;
	padding-left : 10px;
	margin : 5px;
}


QToolTip{
	color : #cccccc;
	background : rgb(6, 6, 6, 200)
}


QScrollBar:horizontal{
	height : 5px;
	border : 0px;
}
QScrollBar::handle:horizontal{
	height : 10px;
	min-width : 10px;
	background : solid grey;
}
QScrollBar::add-line:horizontal{
	width : 0px;
	height : 0px;
}
QScrollBar::sub-line:horizontal{
	width : 0px;
	height : 0px;
}
QScrollBar:vertical{
	width : 5px;
	border : 0px;
}
QScrollBar::handle:vertical{
	height : 10px;
	min-width : 10px;
	background : solid grey;
}
QScrollBar::add-line:vertical{
	width : 0px;
	height : 0px;
}
QScrollBar::sub-line:vertical{
	width : 0px;
	height : 0px;
}


