}


QPushButton{
	font-size : 14px;
	text-align : center; 	
	border : 1px solid #008080;
	background-color : transparent ;
	color : #62C9F3;
}
QPushButton::focus{
	color : #00C9D8;
	border : 1px solid #008080;
}
QPushButton::hover::focus{
	color : #00C9D8;
}
QPushButton::hover{
	color : #A7F9FF;
	border : 2px solid #00aaaa;
}
QPushButton#menuButton{
	border : 0px;
}
QPushButton#menuButton::focus{
	background-color : rgba(255, 255, 255, 10);
	border-radius  :32px;
}
QPushButton#editButton{
	border-radius : 4;
}
QPushButton#sideBarButton{
	border-image : url('Resources/Assets/searchIcon.png');
	background : rgba(6, 6, 6, 10);
	width : 31px;
	height : 31px;
}
QPushButton#sideBarButton::hover{
	border-image : url('Resources/Assets/searchIconActive.png');
	background : rgba(6, 6, 6, 10);
}
QPushButton#sideBarButton::hover::focus{
	border-image : url('Resources/Assets/searchIconActive.png');
	background : rgba(6, 6, 6, 10);
}
QPushButton#sideBarButton::focus{
	border-image : url('Resources/Assets/searchIcon.png');
	background : rgba(6, 6, 6, 10);
}
QPushButton#leftButton{
	border-image:url('Resources/Assets/leftArrow.png');
}
QPushButton#leftButton::hover{
	border-image:url('Resources/Assets/leftArrowActive.png');
}
QPushButton#rightButton{
	border-image:url('Resources/Assets/rightArrow.png');
}
QPushButton#rightButton::hover{
	border-image:url('Resources/Assets/rightArrowActive.png');
}
QPushButton#loginButton{
	background-color : transparent;
	border : 1px solid;
	border-color : #34d989;
	border-radius : 20px;
	font-size : 20px;
	font-family : Adequate;
}
QPushButton#loginButton:focus{
	background-color : rgba(12, 12, 12, 50);
	border : 2px solid;
	border-color : #34d989;
}
QPushButton#loginButton:disabled{
	background-color : rgba(12, 12, 12, 50);
	border : 2px solid;
	border-color : #32dd32;
}



