QTabWidget{
	color : #CCCCCC;
	border : 0px;
	background-color : #1e1c24;
	margin : 0px;
}
QTabWidget::page{
	margin : 0px;
	broder : 0px;
}
QTabWidget#mainTabs QTabBar::tab{
	width : 0px;
	height: 0px;
	border: 0px;
	margin: 0px;
}


QLabel#h1{
	color : #ADFF2F;
	font-size : 42px;
}
QLabel#h2{
	color : #23B2EE;
	font-size : 30px;
}
QLabel#h3{
	color : #999999;
	font-size : 26px;
}
QLabel#h4{
	color : #bbbbbb;
	font-size : 20px;
}
QLabel#repLabel{
	color : #999999;
	font-size : 20px;
}
QLabel#repContent{
	color : #AA2222;
	font-size : 20px;
}
QLabel#memberSinceLabel{
	color : #999999;
	font-size : 20px bold;
}
QLabel#memberSinceContent{
	color : #999999;
	font-size : 20px;
}
QLabel#prestigeLabel{
	font-size : 20px;
	border : 3px solid #AAAAAA;
}


QScrollArea{
	border : 0px;
}
QScrollArea QWidget{
	background-color : #131217;
}


QPushButton{
	font-size : 14px;
	text-align : center; 	
	border : 1px solid #008080;
	background-color : #202428 ;
	color : #62C9F3;
}
QPushButton::focus{
	color : #00C9D8;
	background-color : #2b2c39 ;
	border : 2px solid #008080;
}
QPushButton::hover::focus{
	color : #00C9D8;
	background-color : #2b2c39 ;
}
QPushButton::hover{
	color : #A7F9FF;
	background-color : #202428 ;
	border : 2px solid #00aaaa;
}
QPushButton#menuButton{
	background-color : #00000000;
	border : 0px;
}
QPushButton#menuButton::focus{
	background-color : #00000000;
	border : 1.5px solid #ffe22c; 
	border-radius : 32px;
}
QPushButton#editButton{
	border-radius : 4;
}


QLineEdit{
	height : 30px;
	background : #2d353d;
	color : #aaaaaa;
	border : 1px solid #171b1f;
	padding : 3px;
}
QLineEdit#editableLineEdit{
	background : #1e1c24;
	border : 0px;
	font-size : 20px;
	padding : 0px;
	border-bottom : 0px;
}


QTextEdit{
	background : #1e1c24;
	color : #aaaaaa;
}

QToolTip{
	color : #aaaaaa;
	background : #2d353d;
}


QScrollBar:horizontal{
	height : 0px;
}
QScrollBar:vertical{
	width : 0px;
}